##domain## {
    root * /usr/share/caddy
    file_server
    tls {
        protocols tls1.2 tls1.3
        ciphers TLS_ECDHE_ECDSA_WITH_AES_256_GCM_SHA384 TLS_ECDHE_ECDSA_WITH_CHACHA20_POLY1305_SHA256
        curves x25519
    }

    reverse_proxy ##path## http://localhost:10000 {
        transport http {
            versions h2c 2
        }
    }
    ##reverse_Proxy1##
    ##reverse_Proxy2##
    ##reverse_Proxy3##
}